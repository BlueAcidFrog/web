{
  "name": "ory-website",
  "private": true,
  "dependencies": {
    "gitbook-cli": "^2.3.2"
  },
  "scripts": {
    "start:api": "cd ./docs/api && gitbook serve",
    "start:faq": "cd ./docs/faq && gitbook serve",
    "start:guide": "cd ./docs/guide && gitbook serve",
    "build:api": "cd ./docs/api && gitbook build",
    "build:faq": "cd ./docs/faq && gitbook build",
    "build:guide": "cd ./docs/guide && gitbook build",
    "postinstall": "npm-run-all postinstall:*",
    "postinstall:guide": "cd ./docs/guide && gitbook install",
    "postinstall:api": "cd ./docs/api && gitbook install",
    "postinstall:faq": "cd ./docs/faq && gitbook install"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.2"
  }
}
